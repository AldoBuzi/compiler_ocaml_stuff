def main with input in output out as 
    x := in;
    b := 5;
    a := 0;
    if a < 0 then
    (
        b := in;
    )
    else z := b;
    c := 0;
    p := x + 5;
    out := p;
    while c < 10 do (
        c := c + 1;
        s := c * 2; 
        if s < 7 then
            c := 100;
        else s := s + 1;
    );
    s := out;
    pp := 0;